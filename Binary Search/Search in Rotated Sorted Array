class Solution {
public:
    int search(vector<int>& nums, int target) {
        int st=0,end=nums.size()-1;
        while(st<=end){
            int mid=st+(end-st)/2;
            if(target==nums[mid]){
                return mid;
            }
            if(nums[mid]<=nums[end]){
                if(target>=nums[mid] and target<=nums[end]){
                    st=mid+1;
                }
                else{
                    end=mid-1;
                }
            }
            else if(nums[mid]>=nums[st]){
                if(target>=nums[st] and target<=nums[mid]){
                    end=mid-1;
                }
                else{
                    st=mid+1;
                }
            }
        }
        return -1;
    }
};
