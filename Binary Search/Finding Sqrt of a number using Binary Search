class Solution {
public:
    int mySqrt(int x) {
        int l=1,h=x;
        while(l<=h){
            long long mid=l+(h-l)/2;
            long long sqr=mid*mid;
            if(sqr==x){
                return mid;
            }
            if(sqr>x){
                h=mid-1;
            }
            else{
                l=mid+1;
            }
        }
        return h;
        
    }
};
